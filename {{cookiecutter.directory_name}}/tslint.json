{
    "defaultSeverity": "warning",
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "jsRules": {},
    "rules": {
        "member-ordering": [
            true,
            {
                "order": ["static-field", "instance-field", "static-method", "constructor", "instance-method"]
            }
        ],
        "object-literal-sort-keys": [true, "shorthand-first"]
    },
    "rulesDirectory": []
}
